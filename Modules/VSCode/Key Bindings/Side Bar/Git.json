[
	{
		"key": "ctrl+g ctrl+c",
		"command": "workbench.view.scm"
	},
	{
		"key": "F12",
		"command": "git.pull"
	},
	{
		"key": "shift+F12",
		"command":  "gitlens.pullRepositories"
	},
	{
		"key": "ctrl+g s",
		"command": "git.stage",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g shift+s",
		"command": "git.stageAll",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g u",
		"command": "git.unstage",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g shift+u",
		"command": "git.unstageAll",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g shift+c",
		"command": "git.commitAllSigned",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g a",
		"command": "git.commitStagedAmend",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g shift+a",
		"command": "git.commitAllAmend",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g i",
		"command": "gitlens.showQuickCommitFileDetails",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g ctrl+s",
		"command": "gitlens.showQuickRepoStatus",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g b",
		"command": "gitlens.toggleLineBlame",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g -",
		"command": "gitlens.showLastQuickPick",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g h",
		"command": "gitlens.showQuickFileHistory",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g shift+h",
		"command": "gitlens.showQuickRepoHistory",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g .",
		"command": "gitlens.diffWithNext",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g shift+,",
		"command": "gitlens.diffLineWithPrevious",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g w",
		"command": "gitlens.diffLineWithWorking",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g shift+w",
		"command": "gitlens.diffWithWorking",
		"when": "editorTextFocus && gitlens:enabled"
	},
    {
        "key":  "ctrl+g /",
        "command":  "gitlens.views.search:gitlens.focus",
        "when":  "editorTextFocus && gitlens:enabled"
    },
	{
        "key":  "ctrl+g c",
        "command":  "gitlens.views.compare:gitlens.focus",
        "when":  "editorTextFocus && gitlens:enabled"
	},
    {
		"key":  "ctrl+g f",
        "command":  "gitlens.views.fileHistory:gitlens.focus",
        "when":  "editorTextFocus && gitlens:enabled"
	},
    {
		"key":  "ctrl+g r",
        "command":  "gitlens.views.repositories:gitlens.focus",
        "when":  "editorTextFocus && gitlens:enabled"
	},
    {
		"key":  "ctrl+g r",
        "command":  "gitlens.views.repositories.refresh",
        "when":  "editorTextFocus && gitlens:enabled"
	},
	{
		"key":  "ctrl+g shift+c",
		"command":  "gitlens.views.compare:gitlens.focus",
		"when":  "editorTextFocus && gitlens:enabled"
	},
	{
        "key":  "ctrl+g ctrl+shift+c",
        "command":  "git.cleanAll",
        "when":  "editorTextFocus && gitlens:enabled"
	},

	{
		"key": "ctrl+g shift+b",
		"command": "gitlens.toggleFileBlame",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g /",
		"command": "gitlens.views.search.searchCommits",
        "when":  "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g shift+/",
		"command": "gitlens.showQuickCommitFileDetails",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g d",
		"command": "gitlens.diffLineWithWorking",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g n",
		"command": "gitlens.diffWithNext",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g p",
		"command": "gitlens.diffWithPrevious",
		"when": "editorTextFocus && gitlens:enabled"
	}
]