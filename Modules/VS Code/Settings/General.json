{
	"git.path": "",
	"python.pythonPath": "",
	"markdown.styles": [
		""
	],

	"workbench.startupEditor": "none",
	"workbench.colorTheme": "Day Time",
	"workbench.statusBar.visible": true,
	"workbench.sideBar.location": "right",
	"workbench.activityBar.visible": false,
	"workbench.editor.showTabs": false,
	"workbench.editor.showIcons": true,
	"workbench.editor.tabCloseButton": "off",
	"workbench.editor.labelFormat": "short",
	"workbench.iconTheme": "file-icons-colourless",

	"window.title": "${dirty}Coder",
	"window.titleBarStyle": "custom",
	"window.menuBarVisibility": "toggle",
	"window.enableMenuBarMnemonics": false,
	"window.zoomLevel": 1,

	"zenMode.fullScreen": false,
	"zenMode.centerLayout": true,

	"editor.renderWhitespace": "none",
	"editor.minimap.enabled": false,
	"editor.minimap.showSlider": "mouseover",
	"editor.lineNumbers": "off",
	"editor.cursorBlinking": "solid",
	"editor.cursorStyle": "line",
	"editor.overviewRulerBorder": false,
	"editor.overviewRulerLanes": 0,
	"editor.hideCursorInOverviewRuler": true,
	"editor.renderIndentGuides": false,
	"editor.fontLigatures": true,
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.largeFileOptimizations": false,

	"breadcrumbs.enabled": true,
	"breadcrumbs.symbolPath": "off",

	"explorer.openEditors.visible": 0,
	"explorer.confirmDragAndDrop": false,

	"telemetry.enableTelemetry": false,
	"telemetry.enableCrashReporter": false,

	"update.channel": "none",

	"markdown.preview.linkify": true,
	"markdown.preview.doubleClickToSwitchToEditor": true,

	"terminal.integrated.cursorStyle": "underline",
	"terminal.integrated.cursorBlinking": false,

	"powershell.integratedConsole.showOnStartup": false,
	"powershell.startAutomatically": false,

	"git.autofetch": true,
	"git.autorefresh": true,
	
	"gitlens.codeLens.enabled": false,
	"gitlens.blame.highlight.enabled": false,
	"gitlens.views.repositories.files.layout": "auto",
	"gitlens.views.fileHistory.enabled": false,
	"gitlens.views.lineHistory.enabled": false,
	"gitlens.advanced.messages": {
		"suppressCommitHasNoPreviousCommitWarning": false,
		"suppressCommitNotFoundWarning": false,
		"suppressFileNotUnderSourceControlWarning": false,
		"suppressGitVersionWarning": false,
		"suppressLineUncommittedWarning": false,
		"suppressNoRepositoryWarning": false,
		"suppressResultsExplorerNotice": true,
		"suppressShowKeyBindingsNotice": true
	},
	
	"python.jediEnabled": false,
	
	"markdown.preview.breaks": true,
	
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		
		"**/bin/**": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		
		"**/bin/**": true
	},
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		
		"**/bin/**": true
	},
	
	"jira-plugin.baseUrl": "",
	"jira-plugin.username": "",
	"jira-plugin.workingProject": "",
	"jira-plugin.workingIssueStatues": "In progress",
	"jira-plugin.enableWorkingIssue": true,
	"jira-plugin.trackingTimeMode": "always"
}