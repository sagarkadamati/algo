{
    "workbench.colorTheme": "Solarized Light",
    "workbench.startupEditor": "newUntitledFile",

    "workbench.activityBar.visible": false,
    "workbench.statusBar.visible": true,
    "workbench.editor.showTabs": false,
    "workbench.editor.showIcons": true,
    "workbench.editor.tabCloseButton": "off",
    "workbench.sideBar.location": "right",

    "editor.lineNumbers": "off",
    "editor.minimap.enabled": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "underline-thin",
    "editor.hideCursorInOverviewRuler": true,
    "editor.overviewRulerBorder": false,
    "editor.overviewRulerLanes": 0,

    "window.title": "Coder",
    "window.menuBarVisibility": "toggle",
    "window.zoomLevel": 1,

    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": false,
    "editor.renderIndentGuides": false,

    "workbench.colorCustomizations": {
        "editor.background": "#FDF6E3",
        "editor.foreground": "#657B83",
        "editor.lineHighlightBackground": "#F9F3E2",
        "scrollbarSlider.background": "#FDF6E3",
        "statusBar.background": "#FDF6E3",
        "statusBar.foreground": "#2aa198",
        "activityBar.background": "#FDF6E3",
        "activityBar.foreground": "#2aa198",
        "activityBarBadge.background": "#657B83",
        "activityBarBadge.foreground": "#FDF6E3",
        "terminal.background": "#FDF6E3",
        "terminal.foreground": "#657B83",
        "terminalCursor.background": "#FDF6E3",
        "terminalCursor.foreground": "#657B83",
        "editorGroup.border": "#F9F3E2",
        "scrollbar.shadow": "#FDF6E3",
        "sideBar.background": "#FDF6E3",
        "sideBar.foreground": "#2aa198",
        "sideBarSectionHeader.background": "#FDF6E3",
        "sideBarSectionHeader.foreground": "#2aa198",
        "sideBarTitle.foreground": "#29A197",
        "list.hoverBackground": "#F9F3E2",
        "list.hoverForeground": "#29A197",
        "list.activeSelectionBackground": "#F9F3E2",
        "list.activeSelectionForeground": "#29A197",
        "list.focusBackground": "#F9F3E2",
        "list.focusForeground": "#29A197",
        "list.highlightForeground": "#29A197",
        "list.inactiveFocusBackground": "#F9F3E2",
        "list.inactiveSelectionBackground": "#F9F3E2"
