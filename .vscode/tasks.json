{
	"version": "2.0.0",
	"tasks": [
		{
			"taskName": "build",
			"isBuildCommand": true,
			"showOutput": "always",
			"echoCommand": false,
			"command": "powershell",
			"args": [
				"-ExecutionPolicy",
				"Unrestricted",
				"-NoProfile",
				"-File",
				"${cwd}/scripts/build.ps1",
				"-Build",
				"-FileName",
				"${relativeFile}"
			],
			"problemMatcher": []
		},
		{
			"taskName": "run",
			"isBuildCommand": true,
			"showOutput": "always",
			"echoCommand": false,
			"command": "powershell",
			"args": [
				"-ExecutionPolicy",
				"Unrestricted",
				"-NoProfile",
				"-File",
				"${cwd}/scripts/build.ps1",
				"-FileName",
				"${relativeFile}"
			],
			"problemMatcher": []
		}
	]
},
