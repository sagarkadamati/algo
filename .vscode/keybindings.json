// Place your key bindings in this file to overwrite the defaults
[
	// De-Register Some Default Commands
	{
		"key": "ctrl+p",
		"command": "-workbench.action.quickOpen"
	},
	{
		"key": "ctrl+n",
		"command": "-workbench.action.files.newUntitledFile"
	},
	{
		"key": "ctrl+f",
		"command": "-workbench.action.terminal.focusFindWidget"
	},
	{
		"key": "ctrl+f",
		"command": "-actions.find"
	},
	{
		"key": "ctrl+b",
		"command": "-workbench.action.toggleSidebarVisibility"
	},
	{
		"key": "ctrl+shift+n",
		"command": "-workbench.action.newWindow",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+e",
		"command": "-workbench.action.quickOpen"
	},
	{
		"key": "ctrl+shift+b",
		"command": "-workbench.action.tasks.build"
	},
	{
		"key": "ctrl+shift+e",
		"command": "-workbench.view.explorer"
	},
	{
		"key": "ctrl+shift+w",
		"command": "-workbench.action.closeWindow"
	},
	{
		"key": "ctrl+k f",
		"command": "-workbench.action.closeFolder"
	},
	{
		"key": "ctrl+shift+t",
		"command": "-workbench.action.reopenClosedEditor"
	},
	{
		"key": "ctrl+h",
		"command": "-editor.action.startFindReplaceAction"
	},
	{
		"key": "ctrl+a",
		"command": "-editor.action.selectAll"
	},
	{
		"key": "ctrl+g",
		"command": "-workbench.action.gotoLine"
	},
	{
		"key": "ctrl+k ctrl+h",
		"command": "-workbench.action.output.toggleOutput"
	},


	// Close Action Command
	{
		"key": "ctrl+; ctrl+;",
		"command": "closeFindWidget",
		"when": "editorFocus && findWidgetVisible"
	},
	{
		"key": "ctrl+; ctrl+;",
		"command": "hideSuggestWidget",
		"when": "editorTextFocus && suggestWidgetVisible"
	},
	{
		"key": "ctrl+; ctrl+;",
		"command": "workbench.action.closeQuickOpen",
		"when": "inQuickOpen"
	},


	// Ctrl + P
	{
		"key": "ctrl+p",
		"command": "cursorUp",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+p",
		"command": "cursorUpSelect",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+alt+p",
		"command": "cursorPageUp",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+alt+p",
		"command": "cursorPageUpSelect",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+p",
		"command": "list.focusUp",
		"when": "listFocus && !inputFocus"
	},
	{
		"key": "ctrl+alt+p",
		"command": "list.focusPageUp",
		"when": "listFocus && !inputFocus"
	},
	{
		"key": "ctrl+p",
		"command": "editor.action.previousMatchFindAction",
		"when": "editorFocus && findWidgetVisible"
	},
	{
		"key": "ctrl+p",
		"command": "selectPrevSuggestion",
		"when": "editorTextFocus && suggestWidgetVisible"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenNavigatePreviousInEditorPicker",
		"when": "inQuickOpen && inEditorsPicker"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenNavigatePreviousInFilePicker",
		"when": "inQuickOpen && inFilesPicker"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenNavigatePreviousInRecentFilesPicker",
		"when": "inQuickOpen && inRecentFilesPicker"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenNavigatePrevious",
		"when": "inQuickOpen"
	},


	// Ctrl + N
	{
		"key": "ctrl+n",
		"command": "cursorDown",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+n",
		"command": "cursorDownSelect",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+alt+n",
		"command": "cursorPageDown",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+alt+n",
		"command": "cursorPageDownSelect",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+n",
		"command": "list.focusDown",
		"when": "listFocus && !inputFocus"
	},
	{
		"key": "ctrl+alt+n",
		"command": "list.focusPageDown",
		"when": "listFocus && !inputFocus"
	},
	{
		"key": "ctrl+n",
		"command": "editor.action.nextMatchFindAction",
		"when": "editorFocus && findWidgetVisible"
	},
	{
		"key": "ctrl+n",
		"command": "selectNextSuggestion",
		"when": "editorTextFocus && suggestWidgetVisible"
	},
	{
		"key": "ctrl+n",
		"command": "workbench.action.quickOpenNavigateNextInFilePicker",
		"when": "inQuickOpen && inFilesPicker"
	},
	{
		"key": "ctrl+n",
		"command": "workbench.action.quickOpenNavigateNextInEditorPicker",
		"when": "inQuickOpen && inEditorsPicker"
	},
	{
		"key": "ctrl+n",
		"command": "workbench.action.quickOpenNavigateNextInRecentFilesPicker",
		"when": "inQuickOpen && inRecentFilesPicker"
	},
	{
		"key": "ctrl+n",
		"command": "workbench.action.quickOpenNavigateNext",
		"when": "inQuickOpen"
	},

	// Ctrl + B
	{
		"key": "ctrl+b",
		"command": "cursorLeft",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+b",
		"command": "cursorLeftSelect",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+alt+b",
		"command": "cursorWordStartLeft",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+alt+b",
		"command": "cursorWordStartLeftSelect",
		"when": "editorTextFocus"
	},


	// Ctrl + F
	{
		"key": "ctrl+f",
		"command": "cursorRight",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+f",
		"command": "cursorRightSelect",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+alt+f",
		"command": "cursorWordStartRight",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+alt+f",
		"command": "cursorWordStartRightSelect",
		"when": "editorTextFocus"
	},


	// Line Begining and End
	{
		"key": "ctrl+a",
		"command": "cursorLineStart",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+a",
		"command": "cursorHomeSelect",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+e",
		"command": "cursorLineEnd",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+e",
		"command": "cursorEndSelect",
		"when": "editorTextFocus"
	},


	// Find and Replace
	{
		"key": "ctrl+; ctrl+f",
		"command": "actions.find"
	},
	{
		"key": "ctrl+; ctrl+r",
		"command": "editor.action.startFindReplaceAction"
	},
	{
		"key": "ctrl+; ctrl+shift+r",
		"command": "workbench.action.replaceInFiles"
	},
	{
		"key": "ctrl+; r",
		"command": "workbench.action.files.revert"
	},


	// Editor Commands
	{
		"key": "alt+p",
		"command": "workbench.action.previousEditorInGroup"
	},
	{
		"key": "alt+shift+p",
		"command": "workbench.action.previousEditor"
	},
	{
		"key": "alt+n",
		"command": "workbench.action.nextEditorInGroup"
	},
	{
		"key": "alt+shift+n",
		"command": "workbench.action.nextEditor"
	},


	// UI Commands
	{
		"key": "ctrl+; ctrl+w",
		"command": "editor.action.toggleRenderWhitespace"
	},
	{
		"key": "ctrl+; ctrl+p",
		"command": "workbench.action.quickOpen"
	},
	{
		"key": "ctrl+; p",
		"command": "workbench.action.showCommands"
	},

	{
		"key": "alt+shift+a",
		"command": "workbench.action.toggleActivityBarVisibility",
	},
	{
		"key": "alt+shift+t",
		"command": "workbench.action.toggleTabsVisibility"
	},
	{
		"key": "alt+shift+s",
		"command": "workbench.action.toggleStatusbarVisibility",
	},

	{
		"key": "ctrl+l",
		"command": "workbench.action.toggleSidebarVisibility"
	},
	{
		"key": "alt+e",
		"command": "workbench.view.explorer"
	},
	{
		"key": "alt+shift+e",
		"command": "workbench.files.action.focusOpenEditorsView"
	},
	{
		"key": "alt+d",
		"command": "workbench.view.debug"
	},
	{
		"key": "alt+g",
		"command": "workbench.action.gotoLine"
	},
	{
		"key": "alt+t",
		"command": "workbench.action.terminal.toggleTerminal"
	},
	{
		"key": "alt+o",
		"command": "workbench.action.output.toggleOutput"
	},
	{
		"key": "alt+shift+d",
		"command": "workbench.debug.action.toggleRepl"
	},
	{
		"key": "alt+shift+m",
		"command": "workbench.action.toggleMaximizedPanel"
	},
	{
		"key": "alt+shift+g",
		"command": "workbench.view.scm"
	},
	{
		"key": "alt+shift+x",
		"command": "workbench.view.extensions"
	},


	// Git Commands
	{
		"key": "ctrl+g s",
		"command": "git.stage"
	},
	{
		"key": "ctrl+g shift+s",
		"command": "git.stageAll"
	},
	{
		"key": "ctrl+g u",
		"command": "git.unstage"
	},
	{
		"key": "ctrl+g shift+u",
		"command": "git.unstageAll"
	},
	{
		"key": "ctrl+g c",
		"command": "git.commit"
	},
	{
		"key": "ctrl+g shift+c",
		"command": "git.commitAllSigned"
	},
	{
		"key": "ctrl+g a",
		"command": "git.commitStagedAmend"
	},
	{
		"key": "ctrl+g shift+a",
		"command": "git.commitAllAmend"
	},
	{
		"key": "ctrl+g p",
		"command": "git.pull"
	},
	{
		"key": "ctrl+g shift+p",
		"command": "git.pushTo"
	},
	{
		"key": "ctrl+g r",
		"command": "git.refresh"
	},

	{
		"key": "ctrl+g b",
		"command": "gitlens.toggleFileBlame",
		"when": "editorTextFocus && gitlens:activeIsBlameable"
	},
	{
		"key": "ctrl+g i",
		"command": "gitlens.showQuickCommitFileDetails",
		"when": "editorTextFocus && gitlens:enabled"
	},
	{
		"key": "ctrl+g ctrl+s",
		"command": "gitlens.showQuickRepoStatus",
		"when": "gitlens:enabled"
	},
	{
		"key": "ctrl+g d",
		"command": "gitlens.diffWithPrevious",
		"when": "editorTextFocus && gitlens:activeIsTracked"
	},
	{
		"key": "ctrl+g l",
		"command": "gitlens.toggleCodeLens",
		"when": "editorTextFocus && gitlens:canToggleCodeLens && gitlens:enabled"
	},

	{
		"key": "ctrl+g -",
		"command": "gitlens.showLastQuickPick",
		"when": "gitlens:enabled"
	},
	{
		"key": "ctrl+g /",
		"command": "gitlens.showCommitSearch",
		"when": "gitlens:enabled"
	},
	{
		"key": "ctrl+g h",
		"command": "gitlens.showQuickFileHistory",
		"when": "gitlens:enabled"
	},
	{
		"key": "ctrl+g shift+h",
		"command": "gitlens.showQuickRepoHistory",
		"when": "gitlens:enabled"
	},
	{
		"key": "ctrl+g .",
		"command": "gitlens.diffWithNext",
		"when": "editorTextFocus && gitlens:activeIsTracked"
	},
	{
		"key": "ctrl+g shift+,",
		"command": "gitlens.diffLineWithPrevious",
		"when": "editorTextFocus && gitlens:activeIsTracked"
	},
	{
		"key": "ctrl+g w",
		"command": "gitlens.diffLineWithWorking",
		"when": "editorTextFocus && gitlens:activeIsTracked"
	},
	{
		"key": "ctrl+g shift+w",
		"command": "gitlens.diffWithWorking",
		"when": "editorTextFocus && gitlens:activeIsTracked"
	},


	// Editor Movments
	{
		"key": "ctrl+i",
		"command": "cursorHome",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+; ctrl+e",
		"command": "cursorBottom",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+; ctrl+shift+e",
		"command": "cursorBottomSelect",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+; ctrl+b",
		"command": "cursorTop",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+; ctrl+shift+b",
		"command": "cursorTopSelect",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+d",
		"command": "deleteRight",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+alt+d",
		"command": "deleteWordRight",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+alt+v",
		"command": "workbench.action.toggleCenteredLayout",
		"when": "editorTextFocus"
	}


	// Editor Focus Commands
	{
		"key": "ctrl+; w",
		"command": "workbench.action.closeActiveEditor"
	},
	{
		"key": "ctrl+h",
		"command": "workbench.action.focusFirstEditorGroup",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+h",
		"command": "list.collapse",
		"when": "listFocus && !inputFocus"
	},
	{
		"key": "ctrl+alt+h",
		"command": "list.expand",
		"when": "listFocus && !inputFocus"
	},
	{
		"key": "ctrl+; ctrl+h",
		"command": "workbench.action.moveActiveEditorGroupLeft",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+; ctrl+l",
		"command": "workbench.action.moveActiveEditorGroupRight",
		"when": "editorTextFocus"
	},

	// C_CPP Commands
	{
		"key": "ctrl+; ctrl+i",
		"command": "editor.action.goToDeclaration",
		"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+; ctrl+shift+i",
		"command": "editor.action.goToImplementation",
		"when": "editorHasImplementationProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+; ctrl+d",
		"command": "C_Cpp.GoToDeclaration",
		"when": "editorTextFocus && editorLangId == 'cpp'"
	},
	{
		"key": "ctrl+; ctrl+shift+d",
		"command": "C_Cpp.PeekDeclaration",
		"when": "editorTextFocus && editorLangId == 'cpp'"
	},
	{
		"key": "ctrl+; ctrl+s",
		"command": "C_Cpp.SwitchHeaderSource",
		"when": "editorTextFocus && editorLangId == 'cpp'"
	},
	{
		"key": "ctrl+; ctrl+d",
		"command": "C_Cpp.GoToDeclaration",
		"when": "editorTextFocus && editorLangId == 'c'"
	},
	{
		"key": "ctrl+; ctrl+shift+d",
		"command": "C_Cpp.PeekDeclaration",
		"when": "editorTextFocus && editorLangId == 'c'"
	},
	{
		"key": "ctrl+; ctrl+s",
		"command": "C_Cpp.SwitchHeaderSource",
		"when": "editorTextFocus && editorLangId == 'c'"
	},

	{
		"key": "ctrl+alt+/",
		"command": "workbench.action.gotoSymbol",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+o",
		"command": "workbench.action.quickOpen",
	},
	{
		"key": "ctrl+shift+o",
		"command": "workbench.action.files.openFile",
	},
	{
		"key": "ctrl+k ctrl+k",
		"command": "workbench.action.keepEditor",
	}
]